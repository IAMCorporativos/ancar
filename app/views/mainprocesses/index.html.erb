<p id="notice"><%= notice %></p>

<h1>Listing Mainprocesses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mainprocesses.each do |mainprocess| %>
      <tr>
        <td><%= link_to 'Show', mainprocess %></td>
        <td><%= link_to 'Edit', edit_mainprocess_path(mainprocess) %></td>
        <td><%= link_to 'Destroy', mainprocess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mainprocess', new_mainprocess_path %>
