<p id="notice"><%= notice %></p>

<h1>Listing Subprocesses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subprocesses.each do |subprocess| %>
      <tr>
        <td><%= link_to 'Show', subprocess %></td>
        <td><%= link_to 'Edit', edit_subprocess_path(subprocess) %></td>
        <td><%= link_to 'Destroy', subprocess, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subprocess', new_subprocess_path %>
